{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://raw.githubusercontent.com/ptr727/NxWitness/main/CreateMatrix/Version.schema.json",
  "title": "CreateMatrix Version Schema",
  "definitions": {
    "ProductVersion": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "Name": {
          "type": [
            "string",
            "null"
          ]
        },
        "Stable": {
          "$ref": "#/definitions/VersionUri"
        },
        "Latest": {
          "$ref": "#/definitions/VersionUri"
        }
      }
    },
    "VersionUri": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "Version": {
          "type": [
            "string",
            "null"
          ]
        },
        "Uri": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    }
  },
  "type": "object",
  "properties": {
    "$schema": {
      "type": [
        "string",
        "null"
      ]
    },
    "SchemaVersion": {
      "type": "integer",
      "default": 0
    },
    "Products": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ProductVersion"
      }
    }
  },
  "required": [
    "Products"
  ]
}